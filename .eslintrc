{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["standard", "standard-react", "plugin:prettier/recommended"],
  "env": {
    "browser": true
  },
  "globals": {
    "__PREFIX_PATHS__": false,
    "__PATH_PREFIX__": false,
    "graphql": false
  }
}
