{
  "name": "unindented-gatsby",
  "productName": "Unindented",
  "description": "Home of Daniel Perez Alvarez",
  "homepage": "https://unindented.org",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Daniel Perez Alvarez",
    "email": "daniel@unindented.org",
    "url": "https://unindented.org",
    "github": "unindented",
    "twitter": "unindented"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unindented/unindented-gatsby.git"
  },
  "scripts": {
    "clean:cache": "shx rm -rf .cache",
    "clean:public": "shx rm -rf public",
    "clean": "run-p clean:*",
    "test:lint": "eslint .",
    "test": "run-s test:*",
    "start": "gatsby develop",
    "assets:favicon": "convert static/favicon.png -define icon:auto-resize=\"32,16\" static/favicon.ico",
    "assets": "run-p assets:*",
    "build:public": "gatsby build",
    "build:local": "run-s clean build:public",
    "build:remote": "run-s clean \"build:public -- --prefix-paths\"",
    "build": "run-s build:local",
    "deploy:public": "rsync --archive --delete public/",
    "deploy:dev": "run-s build:remote \"deploy:public -- unindented:/home/unindented/dev.unindented.org/\"",
    "deploy:prod": "run-s build:remote \"deploy:public -- unindented:/home/unindented/unindented.org/\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{css,js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "gatsby": "1.9.252",
    "gatsby-image": "1.0.46",
    "gatsby-link": "1.6.40",
    "gatsby-plugin-canonical-urls": "1.0.17",
    "gatsby-plugin-catch-links": "1.0.19",
    "gatsby-plugin-feed": "1.3.20",
    "gatsby-plugin-google-tagmanager": "1.0.17",
    "gatsby-plugin-manifest": "1.0.19",
    "gatsby-plugin-offline": "1.0.15",
    "gatsby-plugin-react-helmet": "2.0.10",
    "gatsby-plugin-sharp": "1.6.42",
    "gatsby-plugin-styled-components": "2.0.11",
    "gatsby-plugin-typography": "1.7.18",
    "gatsby-remark-copy-linked-files": "1.5.31",
    "gatsby-remark-draw": "1.0.15",
    "gatsby-remark-images": "1.5.61",
    "gatsby-remark-katex": "1.0.13",
    "gatsby-remark-prismjs": "2.0.1",
    "gatsby-remark-responsive-iframe": "1.4.18",
    "gatsby-remark-smartypants": "1.4.12",
    "gatsby-source-filesystem": "1.5.33",
    "gatsby-transformer-remark": "1.7.40",
    "gatsby-transformer-sharp": "1.6.22",
    "mermaid": "8.0.0-rc.8",
    "mermaid.cli": "0.4.6",
    "open-color": "1.6.3",
    "prismjs": "1.14.0",
    "prop-types": "15.6.1",
    "react-helmet": "5.2.0",
    "styled-components": "3.2.6",
    "typography-breakpoint-constants": "0.15.10"
  },
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "eslint": "4.19.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "11.0.0",
    "eslint-config-standard-react": "6.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-standard": "3.0.1",
    "gh-pages": "1.1.0",
    "husky": "0.14.3",
    "lint-staged": "7.0.4",
    "npm-run-all": "4.1.2",
    "prettier": "1.12.1",
    "shx": "0.2.2"
  }
}
